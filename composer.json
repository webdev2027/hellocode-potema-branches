{
  "name": "Ssa/HellocodePotemaBranches",
  "type": "typo3-cms-extension",
  "description": "POTEMA Service Provider",
  "homepage": "https://www.hellotypo3.de/",
  "keywords": [
    "TYPO3",
    "CMS",
    "extension"
  ],
  "authors": [
    {
      "name": "Stefan Sajok",
      "email": "hello@hellotypo3.de",
      "role": "Developer",
      "homepage": "https://www.hellotypo3.de/"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^9.5"
  },
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "autoload": {
    "psr-4": {
      "Ssa\\HellocodePotemaBranches\\": "Classes/"
    }
  },
  "replace": {
    "hellocode_potema_branches": "self.version",
    "typo3-ter/hellocode_potema_branches": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "hellocode_potema_branches "
    }
  }
}
